{
  "name": "boostui",
  "version": "0.0.1",
  "description": "Powerfull components library for Nextjs projects with Material-UI. Bootstraps responsive web applications for any need.",
  "repository": "https://github.com/boostui/boostui.git",
  "author": "imrish",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./layout/Column": "./dist/layout/Column/index.js",
    "./layout/Row": "./dist/layout/Row/index.js",
    "./layout/Section": "./dist/layout/Section/index.js",
    "./theme/ConfigProvider": "./dist/theme/ConfigProvider/index.js",
    "./theme/AuthProvider": "./dist/theme/AuthProvider/index.js",
    "./theme/DialogProvider": "./dist/theme/DialogProvider/index.js",
    "./theme/ScreenProvider": "./dist/theme/ScreenProvider/index.js",
    "./theme/ScrollProvider": "./dist/theme/ScrollProvider/index.js",
    "./theme/__DrawerProvider": "./dist/theme/__DrawerProvider/index.js",
    "./theme/__LocaleProvider": "./dist/theme/__LocaleProvider/index.js",
    "./theme/Docs": "./dist/theme/Docs/index.js",
    "./theme/Page": "./dist/theme/Page/index.js",
    "./theme/Post": "./dist/theme/Post/index.js",
    "./widgets/CTAWidget": "./dist/widgets/CTAWidget/index.js",
    "./widgets/ExpandableList": "./dist/widgets/ExpandableList/index.js",
    "./widgets/Gallery": "./dist/widgets/Gallery/index.js",
    "./widgets/Image": "./dist/widgets/Image/index.js",
    "./widgets/PageHeader": "./dist/widgets/PageHeader/index.js",
    "./widgets/PageHeadline": "./dist/widgets/PageHeadline/index.js",
    "./widgets/SectionTitle": "./dist/widgets/SectionTitle/index.js",
    "./widgets/Stats": "./dist/widgets/Stats/index.js",
    "./widgets/Tabs": "./dist/widgets/Tabs/index.js",
    "./widgets/Testimonials": "./dist/widgets/Testimonials/index.js"
  },
  "scripts": {
    "build": "tsc && babel src --out-dir dist",
    "test": "jest",
    "lint": "eslint src --fix",
    "format": "prettier --write .",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "@mui/material-nextjs": "^6.1.5",
    "@reduxjs/toolkit": "^2.3.0",
    "express": "^4.21.1",
    "next": "^15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.5",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.2.3",
    "@types/next": "^9.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.12.1",
    "@typescript-eslint/parser": "^8.12.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^9.13.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jest": "^29.3.1",
    "prettier": "^3.3.3",
    "typescript": "^4.9.3"
  }
}
