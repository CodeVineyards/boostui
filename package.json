{
  "name": "boostui",
  "version": "0.0.1",
  "description": "Responsive, customizable Material-UI components with integrated context providers for seamless state management.",
  "repository": "https://github.com/yambaskillz/boostui.git",
  "author": "<imrishutush@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && babel src --out-dir dist",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.5",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^8.12.1",
    "@typescript-eslint/parser": "^8.12.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^9.13.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jest": "^29.3.1",
    "prettier": "^3.3.3",
    "typescript": "^4.9.3"
  }
}
